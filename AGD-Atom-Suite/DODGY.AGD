EVENT MAINLOOP1
; Use variable A as a counter
 ADD 1 TO A
 IF A > 10
  SCORE 10
  SUBTRACT 10 FROM A
  LET LINE = 0
  LET COLUMN = 10
  COLOUR 70
  SHOWSCORE
  IF E < 11                                   ; no more than 11 enemy sprites on screen
   ADD 1 TO B
  ENDIF
  IF B > 110

   SPAWN 1 1
   SPAWNED
   READ SETTINGB
   GETRANDOM 128
   LET X = RND
   ADD 16 TO X
   GETRANDOM 128
   LET Y = RND
   ADD 32 TO Y
   IF COLLISION PLAYER
    REMOVE
   ELSE
    SUBTRACT 20 FROM B
    ADD 1 TO E                      ; count enemies.
   ENDIF
   ENDSPRITE
  ENDIF
ENDIF
